{
    "name": "apni-vidya-workspace",
    "version": "1.0.0",
    "description": "Apni Vidya Monorepo Workspace",
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "dev:frontend": "npm run dev --prefix frontend",
        "dev:backend": "npm run start:dev --prefix backend",
        "dev": "npm run dev:frontend",
        "install:all": "npm install",
        "backend:migrate": "npm run prisma:migrate --prefix backend",
        "backend:seed": "npm run prisma:seed --prefix backend"
    },
    "dependencies": {
        "concurrently": "^8.2.2",
        "react-player": "^2.16.1",
        "screenfull": "^6.0.2"
    }
}